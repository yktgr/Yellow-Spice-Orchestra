<% @user.errors.full_messages.each do |msg| %>
  <div><%= msg %></div>
<% end %>

<%= form_for @user do |f| %>
      <%= f.label :image %><%= f.file_field :picture %>
      <%= f.label :name %><%= f.text_field :name %>
      <%= f.label :email %><%= f.email_field :email %>
<%= f.submit %>
<% end %>
<%= link_to '戻る', :back %>
